{
  "name": "redux-crud-store",
  "version": "4.0.0",
  "description": "Store of server-side models for Redux single page applications",
  "main": "src/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "src",
    "es",
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "gulp clean",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run build:es",
    "prepublish": "npm run clean && npm run build && npm run test",
    "postpublish": "git add -A package.json src dist es && git commit -m \"publish v${npm_package_version}\"; bin/tag-with-changelog.sh v${npm_package_version} && git push && git push --tags",
    "posttest": "npm run lint",
    "lint": "flow check && eslint src --ext .flow && eslint test",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-core/register --reporter spec test/*.js",
    "build:commonjs": "gulp commonjs",
    "build:es": "gulp es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uniqueway/redux-crud-store.git"
  },
  "homepage": "https://github.com/uniqueway/redux-crud-store",
  "keywords": [
    "redux",
    "crud",
    "SPA",
    "backbone",
    "cache",
    "models",
    "store"
  ],
  "author": "Devin Howard <devin@callysto.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "cross-env": "^1.0.7",
    "del": "^2.2.0",
    "eslint": "2.10.2",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-import": "1.7.0",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "4.3.0",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.16.0",
    "flow-bin": "^0.25.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "mocha": "^2.2.5",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "immutable": ">= 3.7.6",
    "lodash.isequal": "^4.1.2",
    "redux-saga": ">= 0.9.5"
  }
}
